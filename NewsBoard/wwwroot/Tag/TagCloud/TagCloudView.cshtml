@model TagVMList


<div class="panel panel-default">
    <div class="panel-heading">
        Les tags
    </div>

    <div class="panel-body" style="padding:0px">
        @for (int i = 0; i < Model.Tags.Count; i++)
        {
            <button class="btn btn-info btn-sm"
                    action="@Url.NewsBoardUrlHelper().Action("Tag", "TagCloud", "GetBrowseByTag", new { Id=Model.Tags[i].Id})"
                    actiondata="" type="button" name="simpleGetAction">
                @Model.Tags[i].Label
            </button>
        }
    </div>
</div>


